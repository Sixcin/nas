  "dependencies": {
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@prisma/client": "^4.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "execa": "^7.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "systeminformation": "^5.17.12"
  },
  "description": "NodeNas is a Nas solution based on Node.js.",
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.10",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.3",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "rimraf": "^4.4.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "typescript": "^5.0.2",
    "webpack": "^5.76.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "license": "GPL-3.0",
  "main": "dist/main.js",
  "name": "node-nas",
  "private": true,
  "scripts": {
    "build": "pnpm clean && pnpm build:all",
    "build:all": "pnpm build:format && pnpm build:prisma && nest build && pnpm build:copy",
    "build:copy": "cp -r ../web/dist ./client && cp -r ../doc/docs/.vitepress/dist ./client/docs",
    "build:format": "prettier --write \"src/**/*.ts\"",
    "build:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build:prisma": "pnpm prisma generate",
    "clean": "pnpm rimraf dist client",
    "start": "nest start -e 'node --es-module-specifier-resolution=node'",
    "start:debug": "nest start --debug --watch -e 'node --es-module-specifier-resolution=node'",
    "start:dev": "nest start --watch -e 'node --es-module-specifier-resolution=node'",
    "start:prod": "node --es-module-specifier-resolution=node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "update": "pnpm update --latest"
  },
  "type": "module",
  "version": "0.0.1"
}
